<div class="container" style="margin-top: 60px">
	<center>
		<%= form_for :register, url: register_welcome_index_path do |f| %>
		<p>
			<h2> 注册 </h2>
		</p >
		
		<p>
			<%= f.label :name%>
			<%= f.name_field :name %>
		</p >
		
		<p>
			<%= f.label :email%>
			<%= f.email_field :email %>
		</p >

		<p>
			<%= f.label :password %>
			<%= f.password_field :password %>
		</p >

		<p>
			<%= f.submit%>
		</p >
		<% end %>
	</center>
</div>