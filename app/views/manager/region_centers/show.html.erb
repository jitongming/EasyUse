<div class="container" style="margin-top: 60px">

	<div class="row">
		<div class="col-md-3"  style="margin-top:40px">
			Logo
		</div>

		<div class="col-md-9" style="margin-top:40px">
			<center>
				<h3>单位详情</h3>
			</center>
		</div>
	</div>

	<div class="row">
		<div class="col-md-2" style="padding: 20px">
			<%= render "manager/common/sidebar"%>
		</div>

		<div class="col-md-10" style="padding:20px">
			<h1> <%= notice %> </h1>

			<table class="table table-striped">
				<thead>
					<tr>
						<th colspan="2"></th>
					</tr>
				</thead>
				<tbody>
					<tr>
						<td>区域中心名称</td><td> <%= @manager_region_center.name %></td>
					</tr>

					<tr>
						<td>区域中心描述</td><td><%= @manager_region_center.description %></td>
					</tr>

					<tr>
						<td>区域中心网址</td><td><%= @manager_region_center.url %></td>
					</tr>
				</tbody>
			</table>
			<div class="row pull-right">

				<%= link_to '编辑', edit_manager_region_center_path(@manager_region_center) %> |
				<%= link_to '返回', manager_region_centers_path %>
			</div>
		</div>
	</div>
</div>