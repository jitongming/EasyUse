<h1>Listing b_level_tags</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @b_level_tags.each do |b_level_tag| %>
      <tr>
        <td><%= b_level_tag.name %></td>
        <td><%= b_level_tag.description %></td>
        <td><%= link_to 'Show', b_level_tag %></td>
        <td><%= link_to 'Edit', edit_b_level_tag_path(b_level_tag) %></td>
        <td><%= link_to 'Destroy', b_level_tag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New B level tag', new_b_level_tag_path %>
