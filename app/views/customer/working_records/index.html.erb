<h1>Listing working_records</h1>

<table>
  <thead>
    <tr>
      <th>Start time</th>
      <th>End time</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @working_records.each do |working_record| %>
      <tr>
        <td><%= working_record.start_time %></td>
        <td><%= working_record.end_time %></td>
        <td><%= working_record.content %></td>
        <td><%= link_to 'Show', working_record %></td>
        <td><%= link_to 'Edit', edit_working_record_path(working_record) %></td>
        <td><%= link_to 'Destroy', working_record, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Working record', new_working_record_path %>
