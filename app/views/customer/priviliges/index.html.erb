<h1>Listing priviliges</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @priviliges.each do |privilige| %>
      <tr>
        <td><%= link_to 'Show', privilige %></td>
        <td><%= link_to 'Edit', edit_privilige_path(privilige) %></td>
        <td><%= link_to 'Destroy', privilige, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Privilige', new_privilige_path %>
