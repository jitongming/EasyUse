<h1>Listing maintainer_instruments</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Model</th>
      <th>Price</th>
      <th>Fee per hour</th>
      <th>Img url</th>
      <th>Description</th>
      <th>Status</th>
      <th>User</th>
      <th>Institution</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @maintainer_instruments.each do |maintainer_instrument| %>
      <tr>
        <td><%= maintainer_instrument.name %></td>
        <td><%= maintainer_instrument.model %></td>
        <td><%= maintainer_instrument.price %></td>
        <td><%= maintainer_instrument.fee_per_hour %></td>
        <td><%= maintainer_instrument.img_url %></td>
        <td><%= maintainer_instrument.description %></td>
        <td><%= maintainer_instrument.status %></td>
        <td><%= maintainer_instrument.user_id %></td>
        <td><%= maintainer_instrument.institution_id %></td>
        <td><%= link_to 'Show', maintainer_instrument_path(maintainer_instrument) %></td>
        <td><%= link_to 'Edit', edit_maintainer_instrument_path(maintainer_instrument) %></td>
        <td><%= link_to 'Destroy', maintainer_instrument_path(maintainer_instrument), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Instrument', new_maintainer_instrument_path %>
